<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eduec.tea.anibopi">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="false"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logoanibopi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logoanibopi"
        android:supportsRtl="true"
        android:theme="@style/stiloinicio1">
        <activity
            android:name=".Inicio"
            android:screenOrientation="sensorLandscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Video"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".VideoRocas"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".VideoAnimales"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".Home"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".ActividadAnimales"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".ActividadMontanas"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".VideoPlantas"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".VideoMontanas"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".ActividadPlantas"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".ActividadRocas"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".ActividadEvaluacion"
            android:screenOrientation="sensorLandscape" />
        <activity android:name=".Creditos"
            android:screenOrientation="sensorLandscape"></activity>
    </application>

</manifest>